set term pngcairo enhanced font "helvetica,14" fontscale 1.0 size 800,600

set title "2020 Weight Burndown" enhanced font "helvetica,24"
set border 3

set xdata time
set timefmt "%Y%m%d"
set xrange [ "20200701":"20201231" ]
set format x "%b"
set yrange [ 190:250 ]

set arrow from "20200701",250 to "20201231",190 lw 1 nohead

plot '-' using 1:3 notitle with points pt 2 ps 1
